<script>
    $(document).ready(function() {
        $('.panel').each(function () {
            try {
                $(this).lobiPanel({
                    sortable: true,
                    close: false,
                    editTitle: false,
                    reload: false,
                    // state: "collapsed",
                    minimize: {
                        tooltip: false,
                        icon: 'fa fa-chevron-up',
                        icon2: 'fa fa-chevron-down'
                    },
                    expand:{
                        tooltip: "Maximizar",
                    },
                    unpin: false
                }).on('dragged.lobiPanel', function(ev, lobiPanel){
                    $('.dahsboard-column').matchHeight();
                });
            } catch (err) {}
        });
    });
</script>
<script src="{{ asset('assets/js/app.js') }}"></script>
